{
  "info": {
    "name": "PredictStockPrice API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Predict",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "body": { "mode": "raw", "raw": "{\n  \"symbol\": \"AAPL\",\n  \"horizon\": \"1d\"\n}" },
        "url": { "raw": "http://localhost:8080/api/v1/predict", "protocol": "http", "host": ["localhost"], "port": "8080", "path": ["api","v1","predict"] }
      }
    },
    {
      "name": "History",
      "request": {
        "method": "GET",
        "url": { "raw": "http://localhost:8080/api/v1/history?symbol=AAPL&range=3mo", "protocol": "http", "host": ["localhost"], "port": "8080", "path": ["api","v1","history"], "query": [{"key":"symbol","value":"AAPL"},{"key":"range","value":"3mo"}] }
      }
    },
    {
      "name": "SSE Quotes",
      "request": {
        "method": "GET",
        "url": { "raw": "http://localhost:8080/api/v1/stream/quotes?symbol=AAPL", "protocol": "http", "host": ["localhost"], "port": "8080", "path": ["api","v1","stream","quotes"], "query": [{"key":"symbol","value":"AAPL"}] }
      }
    }
  ]
}
